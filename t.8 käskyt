SELECT DISTINCT name FROM sakila.language ORDER BY name ASC;
SELECT title FROM sakila.film JOIN sakila.film_actor ON film.film_id=film_actor.film_id JOIN sakila.actor ON actor.actor_id=film_actor.actor_id WHERE last_name='Temple';
SELECT first_name, last_name FROM sakila.actor JOIN sakila.film_actor ON actor.actor_id=film_actor.actor_id JOIN sakila.film ON film_actor.film_id=film.film_id WHERE title='Ghost Groundhog';
SELECT count(*) FROM sakila.film JOIN sakila.film_category ON film.film_id=film_category.film_id JOIN sakila.category ON film_category.category_id=category.category_id WHERE name='Horror';
SELECT title FROM sakila.film JOIN sakila.film_category ON film.film_id=film_category.film_id JOIN sakila.category ON film_category.category_id=category.category_id WHERE name='Horror';
