t.5 ja 7 käskyt
SELECT DISTINCT dept_name FROM employees.departments dept_name;
SELECT DISTINCT title FROM employees.titles;
SELECT * FROM employees.salaries ORDER BY salary DESC LIMIT 1;
SELECT MIN(salary) FROM employees.salaries;
SELECT AVG(salary) FROM employees.salaries;
SELECT * FROM employees.employees WHERE last_name='Facello';
SELECT * FROM employees.employees WHERE birth_date>'1949-01-01' AND birth_date<'1960-01-01';
SELECT count(*) FROM employees.employees WHERE gender='M';
SELECT count(*) FROM employees.employees WHERE gender='F';
SELECT * FROM employees.employees ORDER BY last_name LIMIT 10;
SELECT * FROM employees.employees ORDER BY first_name AND last_name ASC LIMIT 10;
SELECT * FROM employees.employees ORDER BY hire_date DESC LIMIT 5;
SELECT first_name, last_name, salary FROM employees.employees JOIN employees.salaries ON employees.emp_no=salaries.emp_no ORDER BY salary DESC LIMIT 1;
SELECT first_name, last_name, salary FROM employees.employees JOIN employees.salaries ON employees.emp_no=salaries.emp_no ORDER BY salary ASC LIMIT 1;
SELECT first_name, last_name, salary FROM employees.employees JOIN employees.salaries ON employees.emp_no=salaries.emp_no WHERE salary>150000;
SELECT count(*) FROM employees.dept_emp JOIN employees.employees ON dept_emp.emp_no=employees.emp_no WHERE dept_no='d007' OR dept_no='d001';
SELECT first_name, last_name, dept_name FROM employees.dept_manager JOIN employees.departments ON dept_manager.dept_no=departments.dept_no JOIN employees.employees ON dept_manager.emp_no=employees.emp_no;
SELECT avg(salary) FROM employees.salaries JOIN employees.dept_emp ON salaries.emp_no=dept_emp.emp_no WHERE dept_no='d007' OR dept_no='d001';

riviin 17 ja 19 olisi myös mahdollista lisätä toinen JOIN johon sisältyisi:
JOIN employees.depertments jossa ON dept_emp.dept_no=departments.dept_no WHERE name='Sales' OR name='Marketing'
